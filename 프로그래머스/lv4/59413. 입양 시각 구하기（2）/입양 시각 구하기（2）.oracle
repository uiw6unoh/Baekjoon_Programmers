SELECT A.HOUR-1, NVL(B.COUNT,0) AS COUNT
FROM (
        SELECT ROW_NUMBER() OVER (ORDER BY NAME) AS HOUR FROM ANIMAL_OUTS
        WHERE ROWNUM <= 24
    ) A
LEFT JOIN (
        SELECT TO_NUMBER(TO_CHAR(DATETIME, 'HH24')) AS HOUR, COUNT(*) AS COUNT
        FROM ANIMAL_OUTS
        WHERE TO_CHAR(DATETIME, 'HH24') BETWEEN 0 AND 23
        GROUP BY TO_CHAR(DATETIME, 'HH24')
        ) B
ON A.HOUR = B.HOUR+1
ORDER BY 1;